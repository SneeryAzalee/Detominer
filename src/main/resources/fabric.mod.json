{
  "schemaVersion": 1,
  "id": "detominer",
  "version": "${version}",
  "name": "Detominer",
  "description": "A standalone 3x3 mining mod with customizations made easy that's tailored for players!",
  "authors": ["sneeryazalee"],
  "contact": {
    "homepage": "https://modrinth.com/datapack/detominer",
    "sources": "https://github.com/SneeryAzalee/Detominer",
    "issues": "https://github.com/SneeryAzalee/Detominer/issues"
  },
  "license": "MIT",
  "icon": "assets/detominer/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "io.sneery.detominer.DetominerDataGenerator"
    ],
    "client": [
      "io.sneery.detominer.client.DetominerClient"
    ],
    "main": [
      "io.sneery.detominer.Detominer"
    ]
  },
  "mixins": [
    "detominer.mixins.json",
    {
      "config": "detominer.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
